class Throwable {
    var[] message;
    var[] stackTrace;

    Throwable(var[] message) {
        self->message = message;
    }

    Throwable(string message) {
        self->message = message.getData();
    }

    fn getStackTrace() : string {
        if(message == null) {
            message = ""; // TodO: fill stack trace
        }

        return message;
    }
}
