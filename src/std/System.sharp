mod std;

import libcore;

/**
 * Created by BraxtonN on 5/23/2017.
 */
class System {

    public static fn print(var[] data) {
        Libcore.os.printData(data);
    }

    public static fn print(string str) {
        Libcore.os.printData(str.value());
    }

    public static fn print(var data) {
        Libcore.os.__print_(data, false);
    }

    public static fn printChar(var data) {
        Libcore.os.__print_(data, true);
    }
}
