mod application;

import io;

class Main {

    public static fn main(String[] args) : int {
        A a, b;
        io.console.write("A() a + b = " + (a+b).value()); // 10

        io.console.write("A() a = " + a.value()); // a = 10
        io.console.write("A() b = " + b.value()); // b = 5
        return 0;
    }
}

class A {

    public A() {
        var = 5;
    }

    int var;

    fn operator+(A klass) : A {
        var += klass.var;
        return self;
    }

    public fn value() : int {
        return var;
    }
}